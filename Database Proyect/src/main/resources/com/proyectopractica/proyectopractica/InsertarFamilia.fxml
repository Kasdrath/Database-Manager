<AnchorPane fx:id="anchorPane" prefHeight="494.0" prefWidth="485.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.proyectopractica.proyectopractica.controller.insertar.InsertarFamiliaController">
   <children>
      <Pane layoutX="14.0" layoutY="14.0" prefHeight="480.0" prefWidth="458.0">
